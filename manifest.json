{
    "manifest_version": 2,
    "minimum_chrome_version": "38",
    "version": "2.0.32",
    "author": "Flyduino & Alexander Fedorov & Carsten Wache",
    "name": "KISS GUI",
    "short_name": "KISSGUI",
    "description": "",
    "offline_enabled": true,
    "app": {
        "background": {
            "scripts": [
                "eventPage.js"
            ],
            "persistent": false
        }
    },
    "permissions": [
        "clipboardWrite",
        "serial",
        "usb",
        "storage",
        "notifications",
        "http://ultraesc.de/",
        "https://*.github.com/",
        "https://*.githubusercontent.com/",
        "https://*.amazonaws.com/",
        {
            "fileSystem": [
                "write"
            ]
        },
        {
            "usbDevices": [
                {
                    "vendorId": 1155,
                    "productId": 57105
                }
            ]
        }
    ],
    "icons": {
        "128": "images/icon_128.png"
    }
}
