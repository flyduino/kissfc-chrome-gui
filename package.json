{
  "name": "kiss-gui",
  "main": "start.js",
  "version": "2.0.7",
  "single-instance": true,
  "scripts": {
    "start": "node node_modules/gulp/bin/gulp.js debug",
    "buildall": "node node_modules/gulp/bin/gulp.js release --osx64 --win32 --win64 --linux64 --linux32 --chromeos"
  },
  "window": {
    "title": "KISS GUI",
    "icon": "images/icon_128.png",
    "min_width": 900,
    "min_height": 600,
    "fullscreen": false,
    "resizable": true,
    "toolbar": true,
    "frame": true
  },
  "repository": {
    "type": "git",
    "url": "github.com/flyduino/kissfc-chrome-gui"
  },
  "author": "Flyduino, Alexander Fedorov & Carsten Wache",
  "chromium-args": "--child-clean-exit --enable-webgl --ignore-gpu-blacklist --disable-pinch",
  "webkit": {
    "plugin": true
  },
  "devDependencies": {
    "archiver": "^2.0.3",
    "bluebird": "3.4.1",
    "del": "^3.0.0",
    "git-rev-sync": "1.12.0",
    "gulp": "^3.9.1",
    "gulp-appdmg": "^1.0.3",
    "gulp-concat": "~2.6.1",
    "gulp-install": "^1.1.0",
    "jquery-ui-npm": "1.12.0",
    "marked": "^0.3.17",
    "nw": "^0.30.2",
    "nw-builder": "^3.5.4",
    "os": "^0.1.1",
    "platform-dependent-modules": "0.0.14",
    "react-switchery": "1.0.0",
    "run-sequence": "^2.2.0",
    "temp": "^0.8.3"
  },
  "config": {
    "platformDevDependentModules": {
      "darwin": [
        "gulp-appdmg@1.0.3"
      ]
    }
  }
}
